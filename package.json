{
  "name": "vite-resolve-tsconfig-paths",
  "version": "0.0.0",
  "description": "A Vite plugin that resolves TypeScript compilerOptions.paths.",
  "type": "module",
  "main": "dist/plugin.js",
  "module": "dist/plugin.mjs",
  "types": "dist/plugin.d.ts",
  "exports": {
    "types": "./dist/plugin.d.ts",
    "default": "./dist/plugin.js"
  },
  "author": "sebtoombs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sebtoombs/vite-resolve-tsconfig-paths.git"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsup-node",
    "lint": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "version": "./scripts/version.sh"
  },
  "dependencies": {
    "tsconfck": "^3.1.3",
    "tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.10.0",
    "@types/node": "^22.5.4",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-unicorn": "^56.0.1",
    "globals": "^15.9.0",
    "prettier": "^3.3.3",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.4.0",
    "vite": "^6.0.6"
  },
  "peerDependencies": {
    "vite": "*"
  },
  "peerDependenciesMeta": {
    "vite": {
      "optional": true
    }
  },
  "keywords": [
    "vite",
    "plugin",
    "resolve",
    "resolver",
    "tsconfig",
    "paths",
    "typescript",
    "compilerOptions.paths"
  ],
  "files": [
    "src",
    "dist"
  ]
}
